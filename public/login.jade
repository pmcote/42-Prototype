extends layout
head 
	link(rel="stylesheet" href="stylesheets/login.css")
block content
	.container
		.row
			.col-xs-12
				h1 Log In
			.col-xs-12
				input(type='text', placeholder="Username")
			.col-xs-12
				input(type='text', placeholder="Password")
			.col-xs-12
				input.btn.primary(type='Submit', value='Log In',onclick="window.location.href='/view_all'")
				input.btn.primary(type="Submit", value="New Researcher" onclick="window.location.href='/new_user'")